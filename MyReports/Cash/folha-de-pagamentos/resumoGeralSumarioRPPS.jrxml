	<queryString language="SQL">
		<![CDATA[SELECT fx.*, (SELECT SUM(fr.valor_saldo) valor_saldo
	FROM $P!{dbSchema}.fin_sfuncional AS ff  
	INNER JOIN $P!{dbSchema}.fin_rubricas AS fr ON fr.id_cad_servidores = ff.id_cad_servidores AND fr.ano = ff.ano AND fr.mes = ff.mes AND fr.complementar = ff.complementar AND fr.dominio = ff.dominio 
	INNER JOIN $P!{dbSchema}.fin_eventos AS fe ON fr.id_fin_eventos = fe.id
	WHERE $P!{id_cad_centros} AND $P!{id_cad_departamentos} AND $P!{id_cad_servidores} AND $P!{id_cad_cargos} AND $P!{id_cad_locais_trabalho} 
	AND ff.ano = "$P!{ano}" AND ff.mes = "$P!{mes}" AND ff.complementar = "$P!{complementar}" AND fr.valor > 0 AND ff.situacao = 1 AND fe.`id_evento` IN ('994', '997')) valor_saldo
FROM $P!{dbSchema}.fin_faixas fx
WHERE  fx.tipo = 0 AND STR_TO_DATE(fx.data, '%d/%m/%Y') <= LAST_DAY(STR_TO_DATE( $P{dataFaixa} , '%d/%m/%Y'))
ORDER BY STR_TO_DATE(fx.data, '%d/%m/%Y') DESC LIMIT 1]]>
	</queryString>
